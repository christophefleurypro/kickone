{#
|
| Menu links
|-------------
#}
<ul class="nav">
	{% for item in items %}
	<li class="{{ item.class }}">
		<a class="link underline {{ classes }}" {% if item.target %}target="{{ item.target }}" {% endif %} href="{{ item.link }}">{{
			item.title
			}}</a>
		{% if item.get_children %}
			{% include "_includes/menu.twig" with {'items': item.get_children} %}
		{% endif %}
	</li>
	{% endfor %}
</ul>